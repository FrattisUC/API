{
  "name": "Submission",
  "plural": "submissions",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "problemId": {
      "type": "number",
      "required": true
    },
    "userId": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "urlContent": {
      "type": "string",
      "required": true
    },
    "kattisResult": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "problem": {
      "type": "belongsTo",
      "model": "Problem",
      "foreignKey": "problemId",
      "options": {
        "nestRemoting": true
      }
    },
    "submission": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "human": {
      "type": "belongsTo",
      "model": "Human",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
