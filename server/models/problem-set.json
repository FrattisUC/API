{
  "name": "ProblemSet",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "start_time": {
      "type": "date"
    },
    "end_time": {
      "type": "date"
    },
    "restricted_access": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "problems": {
      "type": "hasMany",
      "model": "Problem",
      "foreignKey": "problem_set_id",
      "options": {
        "nestRemoting": true
      },
      "through": "ProblemProblemSet"
    }
  },
  "acls": [],
  "methods": {}
}
