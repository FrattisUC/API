{
  "name": "Problem",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "isProgram": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "description": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "tests": {
      "type": "hasMany",
      "model": "Test",
      "foreignKey": "problemId",
      "options": {
        "nestRemoting": true
      }
    },
    "problemSets": {
      "type": "hasMany",
      "model": "ProblemSet",
      "foreignKey": "problemId",
      "options": {
        "nestRemoting": true
      },
      "through": "ProblemProblemSet"
    },
    "submissions": {
      "type": "hasMany",
      "model": "Submission",
      "foreignKey": "problemId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "mixins": {
    "HasManyIntegrityCheck": {}
  },
  "acls": [],
  "methods": {}
}
